<mapping>
    <bind source="address.line1" target="myAddress.address1" />
    <bind source="address.line2" target="myAddress.address2" />
    <bind source="address.line3" target="myAddress.address3" />
    <bind source="address.city" target="myAddress.addresscity" />
    <bind source="address.state" target="myAddress.addressstate" />
    <bind source="address.zip" target="myAddress.addresszip" />

    <post-processors>
        <processor value="org.viablespark.mapper.ReportResultPostProcessor" />
    </post-processors>

</mapping>